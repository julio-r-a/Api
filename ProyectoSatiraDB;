create database ProyectoSatira;
use ProyectoSatira;

create table Producto(
    Codigo INT PRIMARY key,
    Nombre VARCHAR (25),
    Descripcion VARCHAR (50),
    Marca VARCHAR (30)
);

insert into Producto(Codigo,Nombre,Descripcion,Marca)
values(1,'Califa','Cerveza','Satira');
